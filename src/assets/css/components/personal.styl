.section{
  width:100%;
  height:100%;
  background:#F8FBFF;
  .personal-content {
  width: 1200px;
  margin: 0 auto;
  padding: 40px 0 0;
  min-height:calc(100% - 207px);
  margin-bottom:150px;
  h1{
    width:100%;
    height:70px;
    line-height:70px;
    background:#fff;
    color:#000;
    text-indent:54px;
    font-size:30px;
  }
  .qeCode {
    display:block;
    margin:0 auto;
    width:200px;
    height:200px;
  }
  .main-block {
    background:#fff;
    margin: 50px auto 20px;
    border-radius: 2px;
    padding-bottom: 10px; 
  }
  .block-header{
    height: 40px;
    line-height: 40px
    padding-left: 58px;
    font-size:14px; 
    font-weight:bold;
    background: #EEEEEE;
    span{
      display:inline-block;
      margin-right:5px;
    }
    .tab_nav {
      span {
        display: block;
        text-align: center;
        float: left;
        padding: 0 20px;
        color:#000;
        cursor: pointer;
        margin-right: 10px;
        font-size:14px; 
        &.selected {
          color:#785734;
        }
        span{
          padding:0;
        }
      }
    }
  }
  .table-content {
    td ,th {
      height:35px;
      line-height: 26px;
      display:table-cell; 
      vertical-align:middle;
      font-size:12px;
    }
    tr{
      border-bottom:1px solid #E0E0E0;
    }
    tr:last-child{
      border-bottom:0;
    }
    .tr-left {
      text-align: left;
      color:#9B9B9B;
      text-indent:120px;
    }
    .tr-center {
      padding-left: 50px;
      color:#000;
    }
    .tr-right {
      text-align: right;
      padding-right:85px;
      a{
        color:#623E00;
      }
      .QEcode{
        color:#623E00;
      }
    }
    tr.oddback{
      background:#fff;
    }
  }
  .history-table-box {
    min-height: 340px;
  }
  .history-table {
    text-align: left;
    font-size: 12px;
    th {
      color:$explain-font-color;
    }
    .tr-left {
      text-align: left;
      color:#000000;
    }
    .spc{
      th{
        color:#9D9D9D;
      }
    }
    .tr-center {
      padding-left: 70px;
      color:#000000;
    }
    .tr-center-r{
      color:#000000;
    }
    .tr-right{
      color:#000000;
    }
    td {
      width: 24%;
      height: 34px;
    }
  }
  .label-state {
    padding: 0 22px;
    display:inline-block;
    line-height: 38px;
    border-radius: 2px;
    font-size: 16px;
    &.state-not {
      background: $gray-font-color;
    }
    &.state-success{
      background: $yellow-color;
    } 
  }
  .edit-name-input {
    text-indent: 10px;
    width: 258px;
  }
  .disabled-input {
    border: none;
    cursor: default;
    background : none;
    text-indent: 0;
  }
  .yqm-url {
    width: 490px;
    white-space:normal;
    word-break:break-all;
    font-size: 12px;
    color:#9A9A9A;
  }
  .red-font {
    color: $red-color;
    font-size: 12px;
  }
  .switch-btn {
    display: inline-block;
    height: 24px;
    border-radius: 10px;
    position: relative;
    line-height: 24px;
    font-size: 12px;
    background: $base-background-color;
    color: $blue-font-color;
    width: 44px;
    padding: 0 8px 0 24px;
    cursor: pointer;
    overflow: hidden;
    vertical-align: middle;
    .icon {
      position: absolute;
      height: 20px;
      width: 20px;
      border-radius: 50%;
      background: $blue-font-color;
      top: 2px;
      left: 2px;
    }
    &.selected {
      background: $blue-font-color;;
      color: $base-font-color;
      text-align: left;
      width: 40px;
      padding: 0 24px 0 8px;
      .icon {
        right: 2px;
        left: auto;
        background: $base-font-color;
      }
    }
  }
  #payment-block {
    .block-header {
      .text {
        font-size: 12px;
        padding-left 20px;
        color:$explain-font-color;
      }
      .add-payment-btn {
        float: right;
        margin-right: 55px;
      }
    }
    .table-content {
      .tr-left {
        text-align: left;
        line-height: 35px;
        padding-left: 70px;
        img  {
          margin-top: 3px;
          float: left;
          vertical-align: middle;
          border-radius: 100%;
        }
        .title {
          margin-left: 10px;
          float: left;
          line-height: 25px;
          margin-top: 3px;
        }
      }
      .tr-center {
        padding-left: 20px;
        span {
          margin-right: 10px;
        }
      }
      .tr-right {
        a {
          margin-left: 10px;
          vertical-align: middle;
        }
      }
    }
  }
}
}
#payment {
  .upload-img-block {
    .tit {
      display: none;
      float: none;
      padding: 0;
      width: 100%;
      color: #7080a3;
      font-size: 12px;
      margin: 20px 0 10px;
      text-align: left;
    }
    .upload-main {
      padding: 0;
      margin-top: 15px;
      .upload-btn {
        float: left;
      }
      .remarks {
        padding: 14px 0 0 160px;
      }
      .select-fileBtn {
        height: 170px;
        width: 100%;
        line-height: 170px;
        margin-top: 10px;
      }
    }
  }
}