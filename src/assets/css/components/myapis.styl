.myapi{
    .qrcode{
      display: inline-block;
      width: 200px;
      height 200px;
      img{
        width: 120px;
        height:120px;
        margin:10px 0 30px 0
      }
    }
    .error-info{
      text-align: left 
      }
.api-contenr {
  .content{
    margin-top:30px;
    h1{
      width:100%;
      height:40px;
      line-height:40px;
      background:#EEEEEE;
      color:#000;
      text-indent:54px;
      font-size:14px;
    }
    .center{
      display:flex;
      background:#fff;
      box-sizing:border-box;
      padding:20px 70px 80px 70px;
      .left{
        width:35%;
      }
      .right{
        width:65%;
        box-sizing:border-box;
        padding:5% 0 0 10%;
        .prompt {
          width:600px;
          line-height:30px;
          font-size: 14px;
          color:#000;
          text-align: center;
        }
      }
    }
  }
  .loading{
    width:30px;
    height 30px;
    margin: 5% auto
    }

  /* 新建api */
  .new-api {
    width: 263px;
    height: 40px;
    line-height: 40px;
    border-radius: 5px;
    text-align:center;
    cursor: pointer;
    background: #FDDB1A;
    color: #5F3800;
    margin-top:15%;
  }
  .api-list {
    width:100%;
    background:#fff;
    border-top:30px solid #F8FBFF;
    table {
      width: 100%;
      padding-bottom:20px;
      tbody{
        width:100%;
        tr{
          text-align:center;
          line-height:30px;
          border-bottom:1px solid #e1e1e1;
        }
      }
    }

    .head {
      font-size: 14px;
      color: #000;
      line-height:30px;
      border-bottom:1px solid #999
    }
    th, td {
      padding: 10px 0;
    }

    td {
      span {
        margin: 0 5px;
        cursor: pointer;
        color: $blue-font-color

      }
    }
    .no_data{
      height:100px;
      line-height:100px;
      text-align:center;
      font-size:14px;
      color:#000;
    }
  }
}


}