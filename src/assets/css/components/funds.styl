	}
	.fund-manage-nav li,.finance-manage-nav li,.address-manage-nav li{
		width: 120px;
		height: 40px;
		line-height: 40px;
		float: left;
    	text-align: center;
    	font-size: 14px;
    	cursor: pointer;
		}
	/*选中状态*/
	.fundActive{
		color: $blue-font-color;
		border-bottom: 2px solid $blue-font-color;
	}
	div.fundHidden{
		display: block;
	}
	
	/*弹窗样式*/
	.display{
		display: none;
	}
	.dialog-pop .dialog-body{
	// top:15%
	}
	.remindDiv{
		width: 100%;
		height: 100%;
		position: fixed;
		left: 0;
		top: 0;
		background: rgba(0,0,0,0.5);
		z-index: 10;
		overflow-y: auto;
	}
	.remindDiv .confirm-withdraw,.remindDiv .funds-transfer{
		width: 500px;
		border-radius: 2px;
		position: absolute;
		top: 10%;
		left: 50%;
		z-index: 11;
		overflow: hidden;
		margin-left: -250px;
		border:1px solid $border-blue-color;
	}
	.title{
		background-color: $heade-background-color;
		position: relative;
		height: 40px;
		border-bottom: 2px solid $blobk-line-color ;
	}
	.title h6{
		font-size: 14px;
		line-height: 40px;	
		text-indent: 20px;
	}
	.title h5{
		text-indent:27px;
		font-size: 14px;
		line-height: 40px;
	}
	.title h5 i{
		display: inline-block;
		height: 18px;
		width: 18px;
		margin-right: 10px;
		vertical-align: middle;
	}
	.title .close{
		position: absolute;
		top: 10px;
		right: 10px;
		font-size: 14px;
	}
	.infor-list,.transfer-list{
    	overflow: hidden;
	}
	.dialog-content .transfer-list {
		li {
			float: none;
			.select-option-box {
				width:100%;
				padding-bottom:20px;
				.current{
					background-color:#FDDB1A;
					color:#613601;
				}
			}
		}
		li:hover{
			background:#fff;
			color:#613601;
		}
	}
	.transfer-list li,.infor-list li{
		position: relative;
		float: left;
		width: 99%;
		line-height: 42px;
 	}
	.infor-list li span,.transfer-list li span{
		font-size: 14px;
    	margin-right: 5px; 	
	}
	li.warn-txt{
		color:$red-color;
	}
	li.warn-txt i{
		display: inline-block;
		height: 18px;
		width: 18px;
		vertical-align: middle;
		margin-right: 10px;
	}
	.transferOpe input{
	    display: inline-block;
		width: 220px;
		padding-left: 10px;
		margin-right: 10px;
		margin-top:5px;
	}
	.transfer-list li div a {
		float: right;	
		margin-right:40px;
	}
	.transfer-list li div i{
		margin-right: 5px;
	}
	.transfer-list li span:first-child{
		text-align: right;
		display: inline-block;
		margin-right: 15px;
		width: 80px;
		
	}
	.transfer-list li span.usable-quantity{
		height: 42px;
		float: left;
	}

	.transfer-list li input{
	}
	.current-acc{
		display: inline-block;
		cursor: pointer;
		height: 32px;
		width: 100%;
		line-height: 32px;
		border: 1px solid $blue-font-color;
		text-align: center;
		border-radius: 5px;
		margin-right: 20px;
		background: $input-background;
	}
	.infor-list li.error-note,.transfer-list li.error-note{
		color: $red-color;
		height: auto;
		line-height: 30px;
		margin: 0;
	}
	li.last{
