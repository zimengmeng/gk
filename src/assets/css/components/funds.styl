    	margin-top: 15px;
	}
	.cancel,.confirm{
			display: inline-block;
    	height: 40px;
    	width: 100px;
    	text-align: center;
    	line-height: 40px;
    	font-size: 14px;
    	border-radius: 2px;
    	cursor: pointer;
    	margin: 0 18px;
	}
	.cancel{
	}
	.confirm{

	}
