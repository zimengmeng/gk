	  color:$red-color
	  font-size:16px
	}
	.triangleL,.triangleR{
		position: absolute;
    right: 12px;
    top: 25px;
    width: 0;
    height: 0;
    border: 8px solid transparent;
    border-bottom-color: $blue-font-color;
		z-index:10;
	}
	.triangleL:after,.triangleR:after{
		content: '';
    width: 0px;
    height: 0px;
    border: 10px solid transparent;
    border-bottom-color: $sub-background-color;
    position: absolute;
    left: -10px;
    top: -8px;
	}
