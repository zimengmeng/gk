									.fund-manage-hidden{
										color:#E3E3E3;
										font-size:14px;
									}
								}
						}
						/*右侧*/
						.funds-content table{
							width: 100%;
							text-align: center;
							.noWidth{
								color:#CECECE;
								font-size:14px;
							}
							tr{
								border-bottom: 1px solid #EEEEEE;
								line-height:60px;
								td{
									color:#000;
									font-size:14px;
								}
								td a{
									&.deff-text{
										display:blobk;
										box-sizing:border-box;
										padding:6px 5px;
										width:56px;
										height:24px;
										line-height:24px;
										color:#62350C;
										font-size:12px;
										background:#FDDB1A;
									}
									&.deff-texts{
										display:blobk;
										box-sizing:border-box;
										padding:6px 5px;
										width:56px;
										height:24px;
										line-height:24px;
										color:#fff;
										font-size:12px;
										background:#3EABFE;
									}
								}
								td img{
									display: inline-block;
									vertical-align: middle;
									margin-right: 5px;
									width:16px;
									height:16px;
								}
								td.firstTd{
									text-align: left;
									text-indent:50px
								}
								th.firstTh{
									text-align: left;
									text-indent:70px;
								}
								th.lastTh{
									text-align: left;
									text-indent:50px;
								}
								td.noMessage{
									height:480px;
									line-height:480px;
									font-size:14px
								}
								td .rechargeCon{
										padding: 0px 50px;
										border: 1px solid $blue-font-color ;
										position: relative;
										ul{
											float: left;
											margin-right: 30px;
											margin-top:20px;
											li{
												text-align: left;
												font-size: 14px;
												line-height: 30px;
											}
										}
										.qrcode{
											height: 100px;	
											width: 100px;	
											float: left;	
											background-size: cover;	
											margin-right: 30px;	
											margin-top: 20px;	
											.qrcodeimg{
												height: 100px;
												width: 100px;
												float: left;
											}
										}
								}
							}
						}
					}
				}
		}	
	/*tab切换*/
	.loading{
	 height:30px;
	 width:51px;
	 margin:25% auto
	}
	.rechargeLoading{
     height:30px;
	 width:51px;
